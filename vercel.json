{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/vercel.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server/vercel.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    }
  ],
  "functions": {
    "dist/server/vercel.mjs": {
      "maxDuration": 30
    }
  }
}
